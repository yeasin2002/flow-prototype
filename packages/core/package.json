{
  "name": "@flow/core",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./builds": {
      "types": "./dist/builds/index.d.ts",
      "import": "./dist/builds/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./config": {
      "types": "./dist/types/config.d.ts",
      "import": "./dist/types/config.js"
    }
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "h3": "^1.11.0",
    "nitro": "^3.0.0",
    "pathe": "^1.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.22.0",
    "vinxi": "^0.5.8",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.0"
  }
}